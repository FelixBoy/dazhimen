<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dazhimen.bg.bean.Util">
    <select id="getSeqNextVal" resultType="dazhimen.bg.bean.SingleValueBean">
        select nextval(#{seqName}) as valueinfo
    </select>

    <select id="getDBDate" resultType="dazhimen.bg.bean.DBDateTimeBean">
        select now() as dbdate
    </select>
</mapper>